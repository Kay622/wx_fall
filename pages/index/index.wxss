/**index.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
  background-image: url('data:image/jpeg;base64,');
  background-repeat: no-repeat; 
  /* background-size: auto 100%; */
  /* -moz-background-size: auto 100%; */
  background-size: contain;
  background-position:center;
  background-color: black;
} 

.container .content, .my-word {
  position: absolute;
  color: white;
  padding-top: 30rpx;
  font-size: 36rpx;
}

.container .my-word {
  bottom: 50%;
  transform: translateY(546rpx);
}

.content div, .my-word div {
  display: block;
  text-align: center;
}